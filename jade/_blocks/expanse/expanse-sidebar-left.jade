aside.expanse__nav.nav-expanse
	.nav-expanse__header
		.nav-expanse__logo
			img(src="img/logo.svg" alt="alt")

		button.toggle-mnu.on
			span

	- var menu = ['Запрос', 'Предложения', 'Чат'];
	ul.expanse-menu
		each item, ind in menu
			li.expanse-menu__item
				a(href="#" class=ind+1==page ? "expanse-menu__link active" : "expanse-menu__link")= item
